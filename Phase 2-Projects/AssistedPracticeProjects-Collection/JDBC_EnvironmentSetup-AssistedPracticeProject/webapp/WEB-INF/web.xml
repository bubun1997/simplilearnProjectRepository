<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>JDBCSetup</display-name>
  <welcome-file-list>
    <welcome-file>Index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>DemoJdbc</display-name>
    <servlet-name>DemoJdbc</servlet-name>
    <servlet-class>com.soumya.servlet.DemoJdbc</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DemoJdbc</servlet-name>
    <url-pattern>/initialize</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CloseConnectionServlet</display-name>
    <servlet-name>CloseConnectionServlet</servlet-name>
    <servlet-class>com.soumya.servlet.CloseConnectionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CloseConnectionServlet</servlet-name>
    <url-pattern>/close</url-pattern>
  </servlet-mapping>
</web-app>